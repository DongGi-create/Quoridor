<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GameBoardView">
        <attr name="boardColor" format="color"/>
        <attr name="wallColor" format="color"/>
        <attr name="pieceColor" format="color"/>
        <attr name="edgeColor" format="color"/>
        <attr name="recentWallColor" format="color"/>
        <attr name="candidateWallColor" format="color"/>
        <attr name="candidateMoveColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="MenuIconView">
        <attr name="resourceId" format="reference"/>
        <attr name="icon_text" format="string|reference"/>
    </declare-styleable>

    <declare-styleable name="RankingView">
        <attr name="attrMedal" format="reference"/>
        <attr name="attrRank" format="string|reference"/>
        <attr name="attrName" format="string|reference"/>
        <attr name="attrRating" format="string|reference"/>
    </declare-styleable>

</resources>